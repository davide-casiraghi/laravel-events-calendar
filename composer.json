{
    "name": "davide-casiraghi/laravel-events-calendar",
    "description": "Create and manage events in your Laravel application.",
    "keywords": [
        "davide-casiraghi",
        "laravel-events-calendar"
    ],
    "homepage": "https://github.com/davide-casiraghi/laravel-events-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Davide Casiraghi",
            "email": "davide.casiraghi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "anhskohbo/no-captcha": "^3.2",
        "astrotomic/laravel-translatable": "^11.1",
        "davide-casiraghi/laravel-form-partials": "^2.0",
        "fzaninotto/faker": "^1.9",
        "illuminate/support": "^7.0",
        "intervention/image": "^2.4",
        "mcamara/laravel-localization": "^1.5.0",
        "mews/purifier": "^3.2.1",
        "nesbot/carbon": "^2.16",
        "spatie/laravel-honeypot": "^2.2"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.5.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "DavideCasiraghi\\LaravelEventsCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCasiraghi\\LaravelEventsCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavideCasiraghi\\LaravelEventsCalendar\\LaravelEventsCalendarServiceProvider"
            ],
            "aliases": {
                "LaravelEventsCalendar": "DavideCasiraghi\\LaravelEventsCalendar\\LaravelEventsCalendar"
            }
        }
    }
}
